{
  "name": "socketsliders",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "pnpm --filter socketslidersweb dev",
    "build:web": "pnpm --filter socketslidersweb build",
    "build:labels": "tsx scripts/build-label-geometry.ts",
    "dev:api": "pnpm --filter @socketsliders/api dev",
    "dev:worker": "pnpm --filter worker dev"
  },
  "pnpm": {
    "overrides": {
      "ioredis": "5.9.0"
    }
  },
  "devDependencies": {
    "tsx": "^4.21.0"
  }
}
